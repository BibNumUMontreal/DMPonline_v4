<h1>Listing project_groups</h1>

<table>
  <tr>
    <th>Project creator</th>
    <th>Project editor</th>
    <th>User</th>
    <th>Project</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @project_groups.each do |project_group| %>
  <tr>
    <td><%= project_group.project_creator %></td>
    <td><%= project_group.project_editor %></td>
    <td><%= project_group.user_id %></td>
    <td><%= project_group.project_id %></td>
    <td><%= link_to 'Show', project_group %></td>
    <td><%= link_to 'Edit', edit_project_group_path(project_group) %></td>
    <td><%= link_to 'Destroy', project_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Project group', new_project_group_path %>
